{
  "name": "webvrrocks",
  "version": "1.0.0",
  "private": "true",
  "description": "WebVR Rocks",
  "main": "server.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "node ./server",
    "prod": "NODE_ENV='production' node ./server",
    "test": "npm run prod",
    "build": "cp -R public _prod && NODE_ENV='production' posthtml -r -i _prod/**/*.html",
    "deploy": "surge _prod -d https://webvrrocks.surge.sh",
    "open": "opn 'http://webvrrocks.surge.sh'",
    "prebuild": "shx rm -rf _prod",
    "preprod": "npm run build",
    "pretest": "npm run build",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "electricity": "github:cvan/electricity#a10db6a",
    "express": "^4.14.0",
    "express-posthtml": "^1.0.4",
    "internal-ip": "^1.2.0",
    "parseurl": "^1.3.1",
    "posthtml": "^0.9.1",
    "posthtml-cli": "github:cvan/posthtml-cli#7fc87ac",
    "posthtml-head-elements": "^0.5.0",
    "posthtml-include": "^1.1.0",
    "posthtml-livereload": "^1.0.0",
    "posthtml-load-config": "^1.0.0",
    "posthtml-stream": "github:posthtml/posthtml-stream",
    "shx": "^0.2.1",
    "tiny-lr": "^1.0.3",
    "yonder": "^0.2.0"
  },
  "devDependencies": {
    "opn": "^4.0.2"
  },
  "engines": {
    "node": "6.9.1"
  },
  "repository": "webvrrocks/webvrrocks",
  "keywords": [
    "webvr",
    "webvrrocks"
  ]
}
